<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">


<LinearLayout
    android:id="@+id/rti5o6uhbqqa"
    android:layout_width="match_parent"
    android:layout_height="105dp"
    android:background="@drawable/cr10bffffff"
    android:padding="10dp"
    android:layout_marginBottom="10dp"
    android:layout_marginHorizontal="18dp"
    android:elevation="25dp"
    android:orientation="horizontal">
    <ImageView
        android:id="@+id/r68nr3fobuv"
        android:layout_width="91dp"
        android:layout_height="84dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/eventsplaceholder"
        android:scaleType="fitXY"
        />
    <LinearLayout
        android:id="@+id/rnekztfwupnt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:paddingBottom="3dp"
        android:orientation="vertical">
        <TextView
            android:id="@+id/rb8oy3t9afmc"
            android:textColor="#25131A"
            android:textSize="14sp"
            android:textStyle="bold"
            android:layout_marginBottom="8dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Deposit paid for venue"
            />
        <LinearLayout
            android:id="@+id/r15ed395yg2u"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/rdtmbzpy45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginEnd="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/rvm39v0w71ni"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginEnd="5dp"
                    android:src="@drawable/locationsmall"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/rlcqhyrd4k3"
                    android:textColor="#8B8688"
                    android:textSize="12sp"
                    android:layout_marginEnd="23dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Maryâ€™s Wedding"
                    />
            </LinearLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <LinearLayout
                android:id="@+id/ru06p946wwc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/cr5bfb01600d"
                android:paddingVertical="6dp"
                android:paddingHorizontal="8dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rqvtpz3m4qub"
                    android:textColor="#FB0160"
                    android:textSize="12sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="$30.00"
                    />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/rboo8xsic1uq"
            android:textColor="#000000"
            android:textSize="9sp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="28th Dec 2025"
            />
    </LinearLayout>
</LinearLayout>

</LinearLayout>