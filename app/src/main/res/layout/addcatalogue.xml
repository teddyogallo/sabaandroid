<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rtmyk6orv0gh"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFF"
        android:paddingBottom="4dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ruggey7fj0bk"
                android:layout_width="match_parent"
                android:layout_height="843dp"
                android:background="#FFFFFF"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/backbuttonimage"
                    android:layout_width="24dp"
                    android:layout_height="24dp"

                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginStart="20dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/backarrow" />
                <HorizontalScrollView
                    android:id="@+id/rq7xjdndvx0o"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="30dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/r8s7sly8t7xi"
                            android:layout_width="62dp"
                            android:layout_height="4dp"
                            android:background="@drawable/cr10ba7be2e"
                            android:layout_marginEnd="13dp"
                            android:orientation="vertical">
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/r2rk01i26jao"
                            android:layout_width="62dp"
                            android:layout_height="4dp"
                            android:layout_marginEnd="13dp"
                            android:background="@drawable/cr10lr270cd01a2891161"
                            android:orientation="vertical"></LinearLayout>


                    </LinearLayout>
                </HorizontalScrollView>
                <LinearLayout
                    android:id="@+id/rw7l0julb6a"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:layout_marginHorizontal="20dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/rrfklra49nms"
                        android:textColor="#25131A"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        android:layout_marginBottom="10dp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Start Service Setup"
                        />
                    <TextView
                        android:id="@+id/rinla7ljd1u"
                        android:textColor="#8B8688"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Please fill the details below to setup a service"
                        />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/rcmaar737wl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="15dp"
                    android:layout_marginStart="22dp"
                    android:orientation="horizontal">
                    <RelativeLayout
                        android:id="@+id/serviceselectedlayout"
                        android:layout_width="73dp"
                        android:layout_height="73dp"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="4dp"
                        android:background="@drawable/circlebordercolour">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/selectedradioview"
                            android:layout_width="67dp"
                            android:layout_height="67dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            app:cardCornerRadius="50dp">

                            <ImageView
                                android:id="@+id/addeventimage"
                                android:layout_width="73dp"
                                android:layout_height="73dp"
                                android:layout_marginEnd="26dp"
                                android:src="@drawable/defaultimage"
                                android:scaleType="fitXY"
                                />

                        </androidx.cardview.widget.CardView>
                    </RelativeLayout>
                    <TextView
                        android:id="@+id/rb4andttzxo5"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        android:layout_marginEnd="26dp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Add service image"
                        />
                    <LinearLayout
                        android:id="@+id/rmrkoweas9yl"
                        android:layout_width="73dp"
                        android:layout_height="73dp"
                        android:orientation="vertical">
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/rbhdwobsz90n"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/s8b868833sw1cr10"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginHorizontal="20dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/r9he5pczrmt"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginEnd="10dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/userplaceholder" />

                    <EditText
                        android:id="@+id/servicename"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="4dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:hint="Enter Service/Vendor Name"
                        android:inputType="text|textNoSuggestions"
                        android:paddingVertical="18dp"
                        android:textColor="#25131A"
                        android:textSize="14sp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rtk9dvcjwlzf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/s8b868833sw1cr10"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginHorizontal="20dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/rhbrotd6j775"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginEnd="10dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/userplaceholder" />
                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/eventtypespinner"

                        android:layout_gravity="center"
                        android:animateLayoutChanges="true"
                        android:overScrollMode="never"
                        android:soundEffectsEnabled="true"
                        android:spinnerMode="dropdown"
                        android:textColor="#25131A"
                        android:textSize="14sp"
                        android:layout_marginEnd="4dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingVertical="18dp"
                        >

                    </androidx.appcompat.widget.AppCompatSpinner>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rgw4f69zamc6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/s8b868833sw1cr10"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginHorizontal="20dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/rkzggfzcxge"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginEnd="10dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/userplaceholder" />

                    <EditText
                        android:id="@+id/eventlocation"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="4dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:hint="Enter event location"
                        android:inputType="text|textAutoComplete"
                        android:paddingVertical="18dp"
                        android:textColor="#25131A"
                        android:textSize="14sp" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/r6qq8k73e6ss1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/s8b868833sw1cr10"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginHorizontal="21dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/rcxqw4jno8ui1"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginEnd="10dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/userplaceholder" />

                    <EditText
                        android:id="@+id/basepriceeditvalue"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="4dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:hint="enter the base price"
                        android:inputType="number"
                        android:paddingVertical="18dp"
                        android:textColor="#25131A"
                        android:textSize="14sp" />
                </LinearLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp">



                    <com.wang.avi.AVLoadingIndicatorView
                        android:id="@+id/progressindicator"

                        style="@style/AVLoadingIndicatorView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center"
                        android:layout_marginStart="192dp"
                        android:visibility="gone"
                        android:layout_marginTop="4dp"
                        android:gravity="center"
                        app:indicatorColor="@color/purplethemelight"
                        app:indicatorName="BallTrianglePathIndicator"

                        />



                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/nextbuttonlayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/cr10lr270cd01a2891161"
                    android:paddingVertical="16dp"
                    android:layout_marginBottom="40dp"
                    android:layout_marginHorizontal="20dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/nextbuttontext"
                        android:textColor="#FFFFFF"
                        android:textSize="16sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Next"
                        />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
