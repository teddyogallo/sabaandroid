<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homeclientFragment"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF">

    <ScrollView
        android:id="@+id/rwkt7uatih28"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/createeventlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="@drawable/sfb005fsw1cr15bffffff"
                android:layout_marginTop="25dp"
                android:layout_marginBottom="20dp"
                android:layout_marginHorizontal="18dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/r17kzwrocf58"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/sfb005fsw1tl15bl15lr270fb0160f703d0"
                    android:paddingVertical="12dp"
                    android:paddingHorizontal="10dp"
                    android:layout_marginEnd="26dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/rs2i8lsuooym"
                        android:layout_width="56dp"
                        android:layout_height="56dp"
                        android:src="@drawable/addeventicon"
                        android:scaleType="fitXY"
                        />
                </LinearLayout>
                <TextView
                    android:id="@+id/r4clmgz9opk9"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Create New Event"
                    />
            </LinearLayout>

            <!-- start of layout for recent events -->
            <LinearLayout
                android:id="@+id/myeventhorizontallistfragment"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginBottom="19dp"
                android:layout_marginStart="18dp"
                android:layout_marginEnd="2dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/r4vauu1uy32j"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="1dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginEnd="20dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/r7yahn8j8k8k"
                        android:textColor="#25131A"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="My Events"
                        />
                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/eventslistrecycler"

                    android:layout_width="match_parent"
                    android:layout_height="108dp"
                    android:layout_gravity="center_horizontal"

                    android:layout_marginTop="2dp">

                </androidx.recyclerview.widget.RecyclerView>
            </LinearLayout>
            <!-- end of layout for recent events -->

            <LinearLayout
                android:id="@+id/rn0b98vj1qzh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="19dp"
                android:layout_marginStart="18dp"
                android:layout_marginEnd="2dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rolxg9dn54z8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="1dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginEnd="20dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/rqn0oyocf68g"
                        android:textColor="#25131A"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Quick Links"
                        />
                </LinearLayout>
                <RelativeLayout
                    android:id="@+id/loadingindicatorlayour"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"


                    >
                    <com.wang.avi.AVLoadingIndicatorView
                        android:id="@+id/progressbar"

                        style="@style/AVLoadingIndicatorView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center"
                        android:layout_marginStart="192dp"
                        android:visibility="gone"
                        android:layout_marginTop="4dp"
                        android:gravity="center"
                        app:indicatorColor="@color/purplethemelight"
                        app:indicatorName="BallTrianglePathIndicator"

                        />
                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/rul9d3myz9bq"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/messagequicklinkbutton"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/cr15bffffff"
                        android:paddingLeft="15dp"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:layout_marginEnd="10dp"
                        android:elevation="4dp"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/roj4vfckfjdk"
                            android:layout_width="76dp"
                            android:layout_height="76dp"
                            android:layout_marginTop="17dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/message_iconempty"
                            android:tint="@color/primary_pink" />
                        <TextView
                            android:id="@+id/r1v7tv8kmaom"
                            android:textColor="#000000"
                            android:textSize="15sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Messages"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/paymentsquicklinkbutton"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/cr15bffffff"
                        android:layout_marginEnd="10dp"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:elevation="4dp"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/r7pb2blep1ks"
                            android:layout_width="76dp"
                            android:layout_height="76dp"
                            android:layout_marginTop="17dp"
                            android:layout_marginStart="14dp"
                            android:src="@drawable/paymenticon_plain"
                            android:scaleType="fitXY"
                            android:tint="@color/primary_pink"
                            />
                        <TextView
                            android:id="@+id/rd82stl6xrwq"
                            android:textColor="#000000"
                            android:textSize="15sp"
                            android:layout_marginStart="15dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Payments"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/fevoritesquicklinkbutton"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:background="@drawable/cr15bffffff"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:elevation="4dp"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/ryj4qb6hvqh8"
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:layout_marginTop="17dp"
                            android:layout_marginBottom="1dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/vendorcolour" />
                        <TextView
                            android:id="@+id/rdma3z9mmc0u"
                            android:textColor="#000000"
                            android:textSize="15sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Saved Vendors"
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/recentactivitiestitlelayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="1dp"
                android:layout_marginBottom="20dp"
                android:layout_marginHorizontal="18dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rf66986yz1ea"
                    android:textColor="#25131A"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Recent activities"
                    />
            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recentactivitiesrecycler"
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:layout_gravity="center_horizontal"

                android:layout_marginTop="5dp">

            </androidx.recyclerview.widget.RecyclerView>

        </LinearLayout>
    </ScrollView>
</FrameLayout>
